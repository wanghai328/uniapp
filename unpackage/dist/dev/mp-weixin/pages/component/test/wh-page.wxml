<view class="container"><view class="swiper-wrap"><swiper class="swiper" circular="{{true}}" indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><view class="swiper-item"><image class="image" src="{{item.url}}" mode="aspectFill"></image><text class="swiper-text">{{item.content}}</text></view></swiper-item></block></swiper><view class="count_view"><text>疯狂抢购中，还剩</text><uni-countdown class="countdown" vue-id="69860d0d-1" day="{{1}}" hour="{{2}}" minute="{{30}}" second="{{0}}" color="#FFFFFF" background-color="#007AFF" border-color="#007AFF" bind:__l="__l"></uni-countdown></view><block wx:for="{{dataList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['jump',['$event']]]]]}}" class="cell-item _div" bindtap="__e"><image class="cell-image" src="{{item.url}}" mode="aspectFill"></image><text>{{item.content}}</text></view></block></view></view>